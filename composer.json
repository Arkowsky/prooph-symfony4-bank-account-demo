{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "api-platform/api-pack": "^1.1",
        "money/money": "^2.0",
        "moneyphp/money": "^3.1",
        "prooph/common": "^4.2",
        "prooph/event-sourcing": "^5.5",
        "prooph/event-store": "^7.3",
        "prooph/event-store-bus-bridge": "^3.2",
        "prooph/event-store-http-middleware": "^0.2.0",
        "prooph/event-store-symfony-bundle": "^0.5.0",
        "prooph/pdo-event-store": "^1.8",
        "prooph/pdo-snapshot-store": "^1.5",
        "prooph/service-bus": "^6.2",
        "prooph/service-bus-symfony-bundle": "^0.7.0",
        "prooph/snapshotter": "^2.1",
        "symfony/console": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/monolog-bundle": "^3.3",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/yaml": "^4.0"
    },
    "require-dev": {
        "behat/behat": "^3.4",
        "behat/mink": "^1.7",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/symfony2-extension": "^2.1",
        "behatch/contexts": "^3.1",
        "phpspec/phpspec": "^4.3",
        "symfony/browser-kit": "^4.0",
        "symfony/dotenv": "^4.0",
        "symfony/web-server-bundle": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "src/Tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
